<%= render 'users/list', user: current_user, book: @book %>

<div class="col-xs-9">
	<h1>Users</h1>
	<table class="table table-striped">
		<tr>
			<th>image</th>
			<th>name</th>
			<th></th>
		</tr>
		<% @users.each do |users| %>
		<tr>
			<td><%= attachment_image_tag users, :profile_image, fallback: "no_image.jpg",size: '40x40' %></td>
			<td><%= users.name %></td>
			<td><%= link_to "Show", user_path(id:users.id), class: "user_#{users.id}" %></td>
		</tr>
		<% end %>
	</table>


</div>
</div>
</div>